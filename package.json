{
  "author": {
    "name": "netop://ウエハ",
    "url": "https://webb.page"
  },
  "dependencies": {
    "@webb/console": "^2020.4.15"
  },
  "description": "Ensure your desired RethinkDB table exists",
  "devDependencies": {
    "@types/rethinkdb": "^2.3.14",
    "@webb/config-prettier": "^2020.4.16",
    "@webb/test": "^2020.4.15",
    "chronver": "^2020.3.9",
    "del-cli": "^3.0.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "rethinkdb-ts": "^2.4.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "updates": "^10.2.7"
  },
  "homepage": "https://github.com/NetOperatorWibby/ensure-table",
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run test:assert && npm run pretty && npm run increment && git add -A :/"
    }
  },
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@webb/ensure-table",
  "prettier": "@webb/config-prettier",
  "repository": {
    "type": "git",
    "url": "https://github.com/NetOperatorWibby/ensure-table"
  },
  "scripts": {
    "build": "npm run cleanup && tsc",
    "cleanup": "del './dist'",
    "increment": "chronver --increment package",
    "pretty": "prettier --write 'dist/**/*.js' 'dist/**/*.ts'",
    "test": "run-s test:*",
    "test:dependencies": "updates --update ./",
    "test:typescript": "tsc",
    "test:assert": "ts-node test/index.ts"
  },
  "types": "dist/index.d.ts",
  "version": "2020.04.16"
}
