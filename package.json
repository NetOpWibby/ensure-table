    "@webb/config-prettier": "^2021.5.28",
    "@webb/test": "*",
    "chronver": "^2020.6.18",
    "del-cli": "^4.0.1",
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "rethinkdb-ts": "^2.4.18",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "updates": "^13.0.4"
  },
  "homepage": "https://github.com/NetOpWibby/ensure-table",
